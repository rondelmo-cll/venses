ntns2(Verb,P):-
   var(P),
   vsp(Verb,Preps),
   (on(to,Preps)
   ;
   on('p-dir',Preps)),
   !.
ntns2(Verb,P):-
   nonvar(P),
   vsp(Verb,Preps),
   (on(P,Preps)
   ;
   on('p-dir',Preps),
   on(P,[below,down,from,up,into,up_to,to,toward,towards,above,below])),
   !.
