subcat_agg(K,E):-
   pred_a(K,_,Sub),
   Sub\=[],
   on(sp/_/_/P/_,Sub),
   (atomic(P), P=E; list(P), on(E,P)).
checklength:-
   findall_poss(Si),
   length(Si,L),
   (5=<L
   ;
   Si=[I-X|_],
   nogen_member(P-Head,X),
   (P\=r;P=r, Head\=it,Head\='It')
   ),
  !.
   
