fsa2(N3-[n-W,v-W], N0-[x-X], N2-[e-C], N1-[av-H], N5-[v-D], N3-[v-W], N0-[x-X], N2-[e-C], N1-[av-H], N5-[v-D]).
fsa2(N2-[x-P], N0-[p-D, s-D], N-[a-M], N1-[av-V, n-V, v-V], N3-[x-P], N2-[x-P], N0-[p-D], N-[a-M], N1-[n-V], N3-[x-P]).
fsa2(N0-[v-X], N3-[av-W], N2-[a-D,v-D], N1-[n-H,v-H], N5-[r-C,w-C], N0-[v-X], N3-[av-W], N2-[a-D], N1-[n-H], N5-[r-C]).
fsa2(N0-[r-X], N3-[v-W], N2-[v-D], N1-[r-H], N5-[n-C,v-C], N0-[r-X], N3-[v-W], N2-[v-D], N1-[r-H], N5-[n-C]).
fsa2(N0-[d-X], N3-[n-W,v-W], N2-[n-D,v-D], N1-[a-H], N5-[n-C,v-C], N0-[d-X], N3-[n-W], N2-[v-D], N1-[a-H], N5-[n-C]).
fsa2(N0-[d-X], N3-[n-W], N2-[n-D,v-D], N1-[a-H], N5-[n-C,v-C], N0-[d-X], N3-[n-W], N2-[v-D], N1-[a-H], N5-[n-C]).
