objprons(them).
objprons(us).
objprons(himself).
objprons(herself).

selectambs(Head,Tags,Tag):-
     Tags=[Tag],
     !.
selectambs(Head,Tags,Best):-
     Tags\=[Tag],
     on(n,Tags),on(v,Tags),
     select_bestgg(Head,Best),
     !.
disambiguate([],[]):-!.
disambiguate([N-A,N1-B|NuoFrase],NuovaFrase):-
    A=[W], W=v-D,
    v(D,_),
    change_verbph(D,B,A1,A2),
    N2 is N + 1,
    change_numbs([N1-B|NuoFrase],NuFrase),
    disambiguate([N-A1,N2-A2|NuFrase],NuovaFrase),
    !.
disambiguate([N-A,N1-B|NuoFrase],[N-A,N1-A2|NuovaFrase]):-
    A=[x-Punct],
    punct(Punct), Punct\='"',
    B=[n-V,v-V],
    verbs_of_saying(V),
    A2=[v-V],
    length(NuoFrase,L),
    L=<5,
    disambiguate(NuoFrase,NuovaFrase),
    !.
disambiguate([B,N-K, N1-K1|NuoFrase],NuovaFrase):-
      K\=[],
      K\=[A],
      on(n-D,K),   
      on(v-D,K),
      \+fct(D),
      \+r(D),
      \+ check_subcn_fact(N),
     (N=0, spy_lower(D,Low,Pol), 
     (Pol=1, Dd=Low; Pol=0, Dd=D);
      N\=0, Dd=D),
     \+ verbgerund(D),
     weigh_length([B,N-K, N1-K1|NuoFrase]),
     disaccess_wsj(Dd,Best),
     transl_tags(Dd,Best,Tags),
     selectambs(Dd,Tags,WTag),
      transl_tags(D,WTag,NTag),
     on(Tag,NTag),
     checkwsj_dis(D,B,Tag,K1),
    disambiguate([B,N-[Tag-D], N1-K1|NuoFrase],NuovaFrase),
    !.
disambiguate([B,N1-K1,N-K|NuoFrase],NuovaFrase):-
      K\=[],
      K\=[A],
      on(n-D,K),   
      on(v-D,K),
      \+fct(D),
      \+r(D),
      B=_-[v-D1|_],
      (supporto(D1);coplbh(D1)),
     (N=0, spy_lower(D,Low,Pol), 
     (Pol=1, Dd=Low; Pol=0, Dd=D);
      N\=0, Dd=D),
     \+ verbgerund(D),
     weigh_length([B,N-K, N1-K1|NuoFrase]),
     disaccess_wsj(Dd,Best),
     transl_tags(Dd,Best,Tags),
     selectambs(Dd,Tags,WTag),
     transl_tags(D,WTag,NTag),
     on(Tag,NTag),
     newcheckwsj_dis(D,B,Tag,K1),
    disambiguate([B, N1-K1,N-[Tag-D]|NuoFrase],NuovaFrase),
    !.
  
disambiguate([A,B,C,D,E,F|NuoFrase],NuovaFrase):-
    fsa1(A,B,C,D,E,F,G,H,I,L,M,N),
    disambiguate([G,H,I,L,M,N|NuoFrase],NuovaFrase),
    !.
disambiguate([A,B,C,D,E|NuoFrase],NuovaFrase):-
    fsa2(A,B,C,D,E,F,G,H,I,L),
    disambiguate([F,G,H,I,L|NuoFrase],NuovaFrase),
    !.
disambiguate([A,B,C,D|NuoFrase],NuovaFrase):-
    fsa3(A,B,C,D,E,F,G,H),
    disambiguate([E,F,G,H|NuoFrase],NuovaFrase),
    !.
disambiguate([A,B,C|NuoFrase],NuovaFrase):-
    fsa(A,B,C,D,E,F),
    disambiguate([D,E,F|NuoFrase],NuovaFrase),
    !.
disambiguate([A,B|NuoFrase],NuovaFrase):-
    fsa4(A,B,D,E),
    disambiguate([D,E|NuoFrase],NuovaFrase),
    !.
disambiguate([N-K|NuoFrase],[N-[Ca-D]|NuovaFrase]):-
      K\=[],
      K\=[A],
    (on(n-D,K),   
     spy_lower(D, LowW, 1),
     hm(D), Ca=n
    ;
    on(nh-D,K),   
     \+ on(a-D,K),   
     spy_lower(D, LowW, 1), Ca=nh
    ;
    on(a-D,K),   
    spy_lower(D, LowW, 1),
    on(a-LowW,K), Ca=a
    ),   
    disambiguate(NuoFrase,NuovaFrase),
    !.
disambiguate([A|NuoFrase],[B|NuovaFrase]):-
    change_morf_cats(A,B),
    disambiguate(NuoFrase,NuovaFrase),
    !.

disambiguate([A|NuoFrase],[A|NuovaFrase]):-
    disambiguate(NuoFrase,NuovaFrase),
    !.

exceptss(A):-
    A\=work_with,!.
change_verbph(A,B,[v-A1],[p-A2]):-
     exceptss(A),
     on(C-D,B),
     (nouncats(C);detss(C);C=d),
     stringof(List,A), on('_',List),
     recover_descs(A,Descs),
     Descs=[A1,A2],
     prepos(A2),
     !.
change_morf_cats([A],[B]):-
    cmcats(A,B),!.
change_morf_cats(A,A).
cmcats(phl-W,p-W).
cmcats(php-W,p-W).
cmcats(pt-W,p-W).

checkwsj_dis(D,B,Tag,K1):-
     Tag=a,
     K1=[A],
     on(T-P,K1),
     (T=n;T=ag),
     !.
checkwsj_dis(D,B,Tag,K1):-
     Tag=n, 
     (B=_-[n-V,v-V],
      !,
      \+ contrs(V, C, N)
      ;
      on(T-P,K1),
      contrs(P, C, N)),
     !.
checkwsj_dis(S,B,Tag,K1):-
    (Tag=n, 
      B\=_-[p-to], B\=_-[r-_|_],
      K1 \= [a-D, c-D, r-D],
     (B=_-[v-V|_], 
      \+supporto(V), 
      \+coplbh(V)
      ;
      K1=[a-V,av-V,n-V,p-V|_],
      mcon(S,'_',D1),
      mcon(D1,V,V1),
      \+ v(V1,_)
      ;
      B\=_-[v-V|_],
      K1\=[a-V,av-V,n-V,p-V|_]
      )
      ;
     Tag=nt, B\=_-[p-to];
     Tag=v, B\=_-[d-_], B\=_-[a-_], B\=_-[p-of]; 
     (B=_-[p-to];B=_-[r-_|_]), Tag\=n,Tag\=nt;
     B=_-[d-Nu], 
     (Tag\=v; integer(Nu))
      ),
     checkK1(Tag,K1),
     !.

checkwsj_dis(S,B,Tag,K1):-
     Tag=n, 
     B=_-[p-to],
     stringof(List,S),
     reverse(List,[s|_]),
     !.

newcheckwsj_dis(D,B,Tag,K1):-
     Tag=n, 
     B=_-[n-V,v-V],
     !,
     \+ contrs(V, C, N),
     !.
newcheckwsj_dis(S,B,Tag,K1):-
    (Tag=n, 
      B\=_-[p-to], B\=_-[r-_|_],
      K1 \= [a-D, c-D, r-D],
     (B=_-[v-V|_], 
       (supporto(V);
         coplbh(V)), \+ contrs(V, C, N),
         on(v-_,K1)
      ;
      K1=[a-V,av-V,n-V,p-V|_],
      mcon(S,'_',D1),
      mcon(D1,V,V1),
      \+ v(V1,_)
      ;
      B\=_-[v-V|_],
      K1\=[a-V,av-V,n-V,p-V|_]
      )
      ;
     Tag=nt, B\=_-[p-to]
     ;
     Tag=v, B\=_-[d-_], B\=_-[a-_],
            K1\=[d-_], K1\=[a-_],
            on(D2-_,K1), \+ dets(D2)
     ; 
     (B=_-[p-to]
      ;
      B=_-[r-_|_]), Tag\=n,Tag\=nt
     ;
     B=_-[d-Nu], 
     (Tag\=v; integer(Nu))
      ),
     checkK1(Tag,K1),
     !.

checkK1(Tag,K1):-
     on(T-P,K1),
     (Tag=n,P\=to,P\='?';Tag\=n),
     alt(Tag,T),
     !.
alt(n,V):-check_vcat(V).
