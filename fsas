vpcs(finish).
vpcs(begin).

verbgerund(P):-
   stringof(Gerund,P),
   reverse(Gerund, [g,n,i|_]),
   lemmatize_dic_v(P,Verb,S),
   (
   once(inf_v(Verb,I))
   ;
   v(Verb,L)
   ),
   !.

